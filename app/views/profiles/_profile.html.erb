<div class="card">
 <div class="card-header">
      <strong>Name:</strong>
     <%= profile.name %>
    
 </div>
 <div class="card-body">
  <div id="<%= dom_id profile %>">
   <%= image_tag(profile.profile_photo,class:"rounded mx-auto ",style: 'height:300px;width:300px;') %>
   <br/><br/>
   

    <p>
     <strong>Bio:</strong>
     <%= profile.bio %>
    </p>
  </div>
  </div>  
  <div class="card-footer">
    Created : <%= profile.created_at %> | Upadate :<%= profile.updated_at%>
  </div>
 </div>
